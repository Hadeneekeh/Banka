{
  "name": "server",
  "version": "1.0.0",
  "description": "A light weight banking app, Banka.",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node server/index.js",
    "test": "set ENV_TEST=true && npm run migrate && nyc mocha --timeout 10000 server/test/test.js --require @babel/register --exit",
    "build": "rm -rf build && mkdir build && babel -d ./build ./server",
    "heroku-postbuild": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "create": "babel-node server/usingDB/migration/createTable.js",
    "drop": "babel-node server/usingDB/migration/dropTable.js",
    "migrate": "npm run drop && npm run create && npm run seeding",
    "seeding": "babel-node server/usingDB/migration/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hadeneekeh/Banka.git"
  },
  "keywords": [
    "Banking"
  ],
  "author": "Kafilat Abdulwahab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hadeneekeh/Banka/issues"
  },
  "homepage": "https://github.com/Hadeneekeh/Banka#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg": "^7.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
